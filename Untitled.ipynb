    "# Scenario 4: Age Verification\n",
    "age = int(input(\"Enter your age: \"))\n",
    "\n",
    "if age < 0:\n",
    "    print(\"Error! Age cannot be negative.\")\n",
    "elif age >= 65:\n",
    "    print(\"You are a senior citizen.\")\n",
    "elif age >= 18:\n",
    "    print(\"You are eligible to vote.\")\n",
    "else:\n",
    "    print(\"You are not eligible to vote.\")"
